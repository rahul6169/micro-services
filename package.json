{
  "name": "@micro-services/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "api-gateway": "npx nx run api-gateway:serve",
    "user-service": "npx nx run user-service:serve",
    "post-service": "npx nx run post-service:serve",
    "user-service-prisma:generate": "npx prisma generate --schema apps/server/user-service/src/prisma/schema.prisma",
    "user-service-prisma:db:push": "npx prisma db push --schema apps/server/user-service/src/prisma/schema.prisma",
    "user-service-postinstall": "npm run user-service-prisma:db:push && npm run user-service-prisma:generate",
    "post-service-prisma:generate": "npx prisma generate --schema apps/server/post-service/src/prisma/schema.prisma",
    "post-service-prisma:db:push": "npx prisma db push --schema apps/server/post-service/src/prisma/schema.prisma",
    "post-service-postinstall": "npm run post-service-prisma:db:push && npm run post-service-prisma:generate"
  },
  "private": true,
  "dependencies": {
    "@apollo/gateway": "^2.10.2",
    "@apollo/subgraph": "^2.10.2",
    "@nestjs/apollo": "^13.1.0",
