    "@prisma/client": "^6.6.0",
    "axios": "^1.6.0",
    "graphql": "^16.10.0",
    "helmet": "^8.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.29.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/cypress": "20.8.0",
    "@nx/eslint": "20.8.0",
    "@nx/eslint-plugin": "20.8.0",
    "@nx/js": "20.8.0",
    "@nx/nest": "^20.8.0",
    "@nx/node": "20.8.0",
    "@nx/react": "20.8.0",
    "@nx/vite": "20.8.0",
    "@nx/web": "20.8.0",
