generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native", "darwin", "debian-openssl-1.1.x"]
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL_POST_SERVICE")
}

model Post {
    id     String @id @default(auto()) @map("_id") @db.ObjectId
    title  String
    userId String @db.ObjectId
}
