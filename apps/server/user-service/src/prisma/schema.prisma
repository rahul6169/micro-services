generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native", "darwin", "debian-openssl-1.1.x"]
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL_USER_SERVICE")
}

model User {
    id    String @id @default(auto()) @map("_id") @db.ObjectId
    name  String
    email String @unique
}
