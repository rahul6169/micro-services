{
  "name": "@micro-services/user-service-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "user-service-e2e",
    "implicitDependencies": [
      "user-service"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/server/user-service-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "user-service:build"
        ]
      }
    }
  }
}
